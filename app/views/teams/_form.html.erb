<%= form_for(@team, remote: true, html: {class: :team_form, class: "form-horizontal", style: "display:inline;" }) do |f| %>
  <div class="modal-body">
  <ul class="errors"></ul>


  <div class="form-group">


    <%= f.label :game_id, class: "col-md-3 control-label" %>
	<div class="col-md-9">
      <%= f.text_field :game_id, class: 'form-control' %>
	</div>


  </div>

  <div class="modal-footer">
    <%= f.submit "Save Team", class: "btn btn-primary" %>
    <%= link_to "Cancel", "#", class: "btn", data: {dismiss: "modal"} %>
  </div>
  </div>
<% end %>
