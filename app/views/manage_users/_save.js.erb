$("ul.errors").html("");
<% if @user.errors.any? %>
  <% @user.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>  
  $('#manage-users-table').DataTable().ajax.reload();

  <% unless @new_password.nil? %>
    $("#inner-global-modal").html("<%= escape_javascript(render 'password_reset_prompt' ) %>")
  <% else %>
    $("#global-modal").modal("hide");
  <% end %>
<% end %>